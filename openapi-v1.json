{
  "openapi": "3.0.3",
  "info": {
    "title": "Jokes Service",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://http-jokes.http-jokes.svc.cluster.local:8080/"
    }
  ],
  "paths": {
    "/tell/joke": {
      "get": {
